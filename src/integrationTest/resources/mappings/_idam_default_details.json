{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "url": "/o/.well-known/openid-configuration"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "jsonBody": {
          "issuer": "http://0.0.0.0:{{request.requestLine.port}}/o",
          "jwks_uri": "http://0.0.0.0:{{request.requestLine.port}}/jwks"
        }
      }
    }
  ]
}
